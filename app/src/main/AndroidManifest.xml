<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.take_out_app">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".IndentActivity"></activity>
        <activity android:name=".ResInfoActivity" />
        <activity android:name=".AddAddressActivity" />
        <activity android:name=".AddressActivity" />
        <activity android:name=".ChangePasswordActivity" />
        <activity android:name=".SearchResultActivity" />
        <activity android:name=".ResMenuActivity" />
        <activity android:name=".RestaurantLoginActivity" />
        <activity android:name=".RestaurantRegisterActivity" />
        <activity android:name=".PerInfoActivity" />
        <activity android:name=".UserActivity" />
        <activity android:name=".FindInfoActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".CartAdapter" />
        <activity android:name=".GoodsDetailsAdapter" />
        <activity android:name=".GoodsClassifyAdapter" />
        <activity
            android:name=".MainActivity"
            android:launchMode="singleTask"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>